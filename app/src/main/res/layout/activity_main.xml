<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.asmarasusanto.myapplication.MainActivity">

    <Button
        android:text="Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Profile"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Teman"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Teman"
        android:layout_below="@+id/Profile"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Proposal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Proposal"
        android:layout_below="@+id/Teman"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Timeline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Timeline"
        android:layout_below="@+id/Pesan"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Pesan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Pesan"
        android:layout_below="@+id/Proposal"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Trip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Trip"
        android:layout_below="@+id/Timeline"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Chat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chat"
        android:layout_below="@+id/Trip"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/logout"
        android:layout_below="@+id/chat"
        android:layout_alignParentStart="true" />
</RelativeLayout>
